{% extends 'base.html.twig' %}

{% block title %}Reclamation{% endblock %}

{% block body %}
    <h1>Reclamation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ reclamation.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ reclamation.name }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ reclamation.mail }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ reclamation.phone }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ reclamation.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_recla_back') }}">back to list</a>

    {{ include('recla/_delete_form.html.twig') }}
{% endblock %}
