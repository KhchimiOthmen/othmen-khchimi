{% extends 'base.html.twig' %}

{% block title %}SymRecipe - tuteur
{% endblock %}

{% block body %}
	<div class="container">
		{% for message in app.flashes('success') %}
			<div class="alert alert-success mt-4">
				{{ message }}
			</div>
		{% endfor %}

		{% for message in app.flashes('danger') %}
			<div class="alert alert-success mt-4">
				{{ message }}
			</div>
		{% endfor %}

		<h1>Tuteur </h1>

		{{ form(form) }}
		</div>
	{% endblock %}
