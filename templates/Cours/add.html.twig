{% extends 'base.html.twig' %}
{% block title %}AjouterCours{% endblock %}
{% block body %}
<div class="text-center">
            <h2 class="section-heading text-uppercase">Ajouter un Cours</h2>
</div>
 
{{form_start(form,{'attr': {'novalidate': 'novalidate'}} )}}
<table>
<tr>

<td> {{ form_label(form.idCours,"Identifiant du cours")  }} 
 </td>
<td>  {{ form_widget(form.idCours) }} 
</td>  
<td> {{ form_errors(form.idCours) }} 
</td>
</tr>

<tr>

<td>  {{ form_label(form.titre,"Titre") }} 
</td> 
<td> {{ form_widget(form.titre)}} 
 </td> 
<td>  {{ form_errors(form.titre) }} 
</td> 

</tr>
<tr>

<td>  {{ form_label(form.description,"Description") }} 
</td> 
<td> {{ form_widget(form.description)}} 
 </td> 
<td>  {{ form_errors(form.description) }} 
</td> 

</tr>
<tr>

<td>  {{ form_label(form.date,"Date") }} 
</td> 
<td> {{ form_widget(form.date)}} 
 </td> 
<td>  {{ form_errors(form.date) }} 
</td> 

</tr>



</table>

 {{ form_widget(form.save)}} 
{{form_end(form)}}

{% endblock %}