{% extends 'base.html.twig' %}
{% block title %}AfficherCours{% endblock %}
{% block body %}
<div class="text-center">
            <h2 class="section-heading text-uppercase">Liste des Cours</h2>
</div>
<table border="4">
    <tr>
        <th>ID</th>
        <th>TITRE</th>
        <th>DESCRIPTION</th>
         <th>DATE</th>
    
    </tr>
    {% for i in cours %}
        <tr>
            <td>{{ i.idCours }}</td>
            <td>{{ i.titre }}</td>
            <td>{{ i.description }}</td>
            <td>{{ i.date|date("d-m-Y")}}</td>
            
        </tr>
    {% endfor %}
</table>
{% endblock %}